<hydraDef>
    
    <!-- Example of a state conversion -->
    <dictionary name="ON_OFF_dict">
        <pair key="0" str="OFF"/>
        <pair key="1" str="ON"/>
    </dictionary>
	<typeState name="ON_OFF_t" size="1" dict="ON_OFF_dict" endian = "LITTLE"/>

	<dictionary name="ENABLE_DISABLE_dict">
        <pair key="0" str="Disabled"/>
        <pair key="1" str="Enabled"/>
    </dictionary>
	<typeState name="Enable_Diable_t" size="1" dict="ENABLE_DISABLE_dict" endian = "LITTLE"/>
    
    <dictionary name="TRUE_FALSE_dict">
        <pair key="0" str="False"/>
        <pair key="1" str="True"/>
    </dictionary>
    <typeState name="TRUE_FALSE_t" size="1" dict="TRUE_FALSE_dict" endian = "LITTLE"/>

    <dictionary name="Yes_No_dict">
        <pair key="0" str="No"/>
        <pair key="1" str="Yes"/>
    </dictionary>
    <typeState name="YES_NO_t" size="1" dict="Yes_No_dict" endian = "LITTLE"/>

    <dictionary name="Good_Bad_dict">
        <pair key="0" str="Bad"/>
        <pair key="1" str="Good"/>
    </dictionary>
    <typeState name="Good_Bad_t" size="1" dict="Good_Bad_dict" endian = "LITTLE"/>

    <dictionary name="Spacecraft_Mode_dict">
        <pair key="0" str="Phoenix"/>
        <pair key="1" str="Safe"/>
        <pair key="2" str="Charging"/>
        <pair key="3" str="Science-CIP"/>
        <pair key="4" str="Science-DAXSS"/>
    </dictionary>
    <typeState name="Spacecraft_Mode_t" size="3" dict="Spacecraft_Mode_dict" endian = "LITTLE"/>

</hydraDef>
