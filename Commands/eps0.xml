<hydraDef>

  <!-- Enable commands for transceiver enables related to EPS -->
  <command name="SetPSENTREN" decoder="cmdDecoder">
    <header frame="SetGPIOCommand_Header"/>
    <description text="Set transceiver enable GPIO for power supply enables"/>
    <field>
      <item name="SetGPIO_OpCode"/>
      <item name="TRANSCEIVER_ENABLES_GPIO_INSTANCE"/>
      <item name="PSEN_TREN"/>
      <item name="GPIO_ON"/>
    </field>
  </command>

